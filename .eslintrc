
{
  "extends": "airbnb",
  "plugins": [
    "react",
    "react-native"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "jest": true,
    "browser": true,
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "react-native/no-unused-styles": 2,
    "react-native/split-platform-components": 2,
    "react-native/no-color-literals": 2,
  }
}
